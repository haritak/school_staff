<p id="notice"><%= notice %></p>

<h1>School Course Teachers</h1>

<table>
  <thead>
    <tr>
      <th>School course</th>
      <th>Teacher</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @school_course_teachers.each do |school_course_teacher| %>
      <tr>
        <td><%= school_course_teacher.school_course %></td>
        <td><%= school_course_teacher.teacher %></td>
        <td><%= link_to 'Show', school_course_teacher %></td>
        <td><%= link_to 'Edit', edit_school_course_teacher_path(school_course_teacher) %></td>
        <td><%= link_to 'Destroy', school_course_teacher, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New School Course Teacher', new_school_course_teacher_path %>
