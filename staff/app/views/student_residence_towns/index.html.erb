<p id="notice"><%= notice %></p>

<h1>Student Residence Towns</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @student_residence_towns.each do |student_residence_town| %>
      <tr>
        <td><%= link_to 'Show', student_residence_town %></td>
        <td><%= link_to 'Edit', edit_student_residence_town_path(student_residence_town) %></td>
        <td><%= link_to 'Destroy', student_residence_town, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Student Residence Town', new_student_residence_town_path %>
