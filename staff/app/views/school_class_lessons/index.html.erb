<p id="notice"><%= notice %></p>

<h1>School Class Lessons</h1>

<table>
  <thead>
    <tr>
      <th>School class</th>
      <th>Lesson</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @school_class_lessons.each do |school_class_lesson| %>
      <tr>
        <td><%= school_class_lesson.school_class %></td>
        <td><%= school_class_lesson.lesson %></td>
        <td><%= link_to 'Show', school_class_lesson %></td>
        <td><%= link_to 'Edit', edit_school_class_lesson_path(school_class_lesson) %></td>
        <td><%= link_to 'Destroy', school_class_lesson, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New School Class Lesson', new_school_class_lesson_path %>
