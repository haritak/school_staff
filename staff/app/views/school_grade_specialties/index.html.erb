<p id="notice"><%= notice %></p>

<h1>School Grade Specialties</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Description</th>
      <th>School Grade</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @school_grade_specialties.each do |school_grade_specialty| %>
      <tr>
        <td><%= school_grade_specialty.code %></td>
        <td><%= school_grade_specialty.description %></td>
        <td><%= school_grade_specialty.school_grade %></td>
        <td><%= link_to 'Show', school_grade_specialty %></td>
        <td><%= link_to 'Edit', edit_school_grade_specialty_path(school_grade_specialty) %></td>
        <td><%= link_to 'Destroy', school_grade_specialty, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New School Grade Specialty', new_school_grade_specialty_path %>
