<p id="notice"><%= notice %></p>

<h1>Saved School Courses</h1>

<table>
  <thead>
    <tr>
      <th>School class</th>
      <th>Lesson</th>
      <th>Duration</th>
      <th>Save no</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @saved_school_courses.each do |saved_school_course| %>
      <tr>
        <td><%= saved_school_course.school_class %></td>
        <td><%= saved_school_course.lesson %></td>
        <td><%= saved_school_course.duration %></td>
        <td><%= saved_school_course.save_no %></td>
        <td><%= link_to 'Show', saved_school_course %></td>
        <td><%= link_to 'Edit', edit_saved_school_course_path(saved_school_course) %></td>
        <td><%= link_to 'Destroy', saved_school_course, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Saved School Course', new_saved_school_course_path %>
