<p id="notice"><%= notice %></p>

<h1>School Students</h1>

<table>
  <thead>
    <tr>
      <th>School</th>
      <th>Student</th>
      <th>Registry no</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @school_students.each do |school_student| %>
      <tr>
        <td><%= school_student.school %></td>
        <td><%= school_student.student %></td>
        <td><%= school_student.registry_no %></td>
        <td><%= link_to 'Show', school_student %></td>
        <td><%= link_to 'Edit', edit_school_student_path(school_student) %></td>
        <td><%= link_to 'Destroy', school_student, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New School Student', new_school_student_path %>
