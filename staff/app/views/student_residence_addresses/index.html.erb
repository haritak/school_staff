<p id="notice"><%= notice %></p>

<h1>Student Residence Addresses</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @student_residence_addresses.each do |student_residence_address| %>
      <tr>
        <td><%= link_to 'Show', student_residence_address %></td>
        <td><%= link_to 'Edit', edit_student_residence_address_path(student_residence_address) %></td>
        <td><%= link_to 'Destroy', student_residence_address, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Student Residence Address', new_student_residence_address_path %>
