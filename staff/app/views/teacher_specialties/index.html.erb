<p id="notice"><%= notice %></p>

<h1>Teacher Specialties</h1>

<table>
  <thead>
    <tr>
      <th>Teacher</th>
      <th>Specialty</th>
      <th>Priority</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teacher_specialties.each do |teacher_specialty| %>
      <tr>
        <td><%= teacher_specialty.teacher.person.last_name %></td>
        <td><%= teacher_specialty.specialty.code %></td>
        <td><%= teacher_specialty.priority %></td>
        <td><%= link_to 'Show', teacher_specialty %></td>
        <td><%= link_to 'Edit', edit_teacher_specialty_path(teacher_specialty) %></td>
        <td><%= link_to 'Destroy', teacher_specialty, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Teacher Specialty', new_teacher_specialty_path %>
